{
    "version": 1,
    "disable_existing_loggers": false,
    "formatters": {
        "standard": {"format": "[%(levelname)s] %(name)s %(funcName)s: %(message)s"}
    },
    "handlers": {
        "default": {
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.StreamHandler",
            "stream": "ext://sys.stdout"
        }
    },
    "loggers": {
        "": {"handlers": ["default"], "level": "INFO", "propagate": false},
        "cryptshare": {"handlers": ["default"], "level": "WARNING", "propagate": false},
        "cryptshare.NotificationMessage": {"handlers": ["default"], "level": "WARNING", "propagate": false},
        "cryptshare.CryptshareApiRequestHandler": {"handlers": ["default"], "level": "INFO", "propagate": false},
        "__main__": {"handlers": ["default"], "level": "WARNING", "propagate": false}
    }
}